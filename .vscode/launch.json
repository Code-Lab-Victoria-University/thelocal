    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Lambda",
            "program": "${workspaceRoot}/node_modules/bespoken-tools/bin/bst-proxy.js",
            "runtimeArgs": ["--inspect-brk=9229"],
            "port": 9229,
            "args": ["lambda", "lambda/dist/index.js"],
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Model",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "debug-model"
            ],
            "port": 9229,
            "sourceMaps": true
        }
    ]
}